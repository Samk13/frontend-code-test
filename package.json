{
  "name": "tre-frontend-code-test",
  "private": true,
  "description": "Tre Frontend Code Test",
  "version": "1.0.0",
  "author": "Hi3G",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "dev:stage": "TARGET=stage gatsby develop",
    "dev:preview": "GATSBY_IS_PREVIEW=true gatsby develop",
    "build:incremental": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "graphql-test-codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "^2.21.37",
    "gatsby-image": "^2.4.6",
    "gatsby-plugin-layout": "^1.3.2",
    "gatsby-plugin-manifest": "^2.4.5",
    "gatsby-plugin-offline": "^3.2.3",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sharp": "^2.6.4",
    "gatsby-plugin-styled-components": "^3.3.2",
    "gatsby-plugin-typegen": "^2.2.0",
    "gatsby-source-filesystem": "^2.3.10",
    "gatsby-source-graphql": "^2.5.2",
    "gatsby-transformer-sharp": "^2.5.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "react-intl": "^4.6.3",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.15.0",
    "@graphql-codegen/fragment-matcher": "1.15.0",
    "@graphql-codegen/introspection": "1.15.0",
    "@graphql-codegen/typescript": "1.15.0",
    "@graphql-codegen/typescript-operations": "1.15.0",
    "@graphql-codegen/typescript-react-apollo": "1.15.0",
    "@types/react": "^16.9.35",
    "@types/react-helmet": "^6.0.0",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "prettier": "^2.0.5",
    "typescript": "^3.9.3"
  }
